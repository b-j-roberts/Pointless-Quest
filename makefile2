PointlessQuest: PointlessQuest.o
	g++ PointlessQuest.o Objects/Tile.o Objects/Player.o Include/Functions.o Objects/World.o Resources/Textures.o Resources/Sprites.o Objects/Plantlife.o -o PointlessQuest -lstdc++ -L C:/SFML-2.5.1/lib -lsfml-graphics -lsfml-window -lsfml-system

PointlessQuest.o: PointlessQuest.cpp Tile.o Player.o
	g++ -c PointlessQuest.cpp -I C:/SFML-2.5.1/include 

Plantlife.o:
	g++ -c Objects/Plantlife.cpp -I C:/SFML-2.5.1/include -o Objects/Plantlife.o

Sprites.o:
	g++ -c Resources/Sprites.cpp -I C:/SFML-2.5.1/include -o Resources/Sprites.o

Textures.o:
	g++ -c Resources/Textures.cpp -I C:/SFML-2.5.1/include -o Resources/Textures.o

World.o:
	g++ -c Objects/World.cpp -I C:/SFML-2.5.1/include -o Objects/World.o

Tile.o:
	g++ -c Objects/Tile.cpp -I C:/SFML-2.5.1/include -o Objects/Tile.o

Player.o:
	g++ -c Objects/Player.cpp -I C:/SFML-2.5.1/include -o Objects/Player.o


Functions.o:
    g++ -c Include/Functions.cpp -I C:/SFML-2.5.1/include -o Include/Functions.o
	
clean:
	rm Player.o Tile.o PointlessQuest.o PointlessQuest.exe
